#include <iostream>
#include <cstdlib>  // for rand() and srand()
#include <ctime>    // for time()

using namespace std;

int main()
{
    int number = 0;

    while (number < 8)
        {
            cout << "Hello\n";
            number++;
        }
        cout << "That's all!\n";
    return 0;
}
